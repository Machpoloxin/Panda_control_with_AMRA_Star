{
  "configurations": [
    {
      "name": "ROS2",
      "includePath": [
        "${workspaceFolder}/**",  // 当前工作区中的所有文件夹
        "/opt/ros/humble/include/**",  // ROS2的全局头文件路径
        "/usr/include/**",  // 系统头文件
        "/usr/include/eigen3/**",  // Eigen3库的头文件
        "/home/xm/pandadev_ws/install/**",  // 你工作空间中的已安装包
        "/home/xm/pandadev_ws/src/panda_description/include/**",  // panda_description 包头文件
        "/home/xm/pandadev_ws/src/amra_star/include/**"  // amra_star 包头文件
      ],
      "compilerPath": "/usr/bin/g++",  // 编译器路径
      "cStandard": "gnu11",  // C语言标准
      "cppStandard": "c++17",  // C++标准
      "intelliSenseMode": "gcc-x64",  // IntelliSense 模式
      "browse": {
        "path": [
          "/opt/ros/humble/include",  // 浏览ROS2的全局头文件
          "/usr/include",  // 浏览系统头文件
          "/usr/include/eigen3",  // 浏览Eigen3头文件
          "/home/xm/pandadev_ws/install",  // 浏览工作空间安装路径
          "/home/xm/pandadev_ws/src"  // 浏览工作空间源码路径
        ],
        "limitSymbolsToIncludedHeaders": false,
        "databaseFilename": "${default}"
      }
    }
  ],
  "version": 4
}